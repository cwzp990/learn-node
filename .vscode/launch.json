// .vscode/launch.json
{
    "version": "0.1.0",
    "configurations": [
      {
        "name": "Launch Egg",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": [ "run", "debug" ],
        "console": "integratedTerminal",
        "restart": true,
        "autoAttachChildProcesses": true
      }
    ]
  }
  